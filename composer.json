{
  "name": "instride/pimcore-js-translation",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "A pretty nice way to expose your Pimcore shared translation messages to your client applications.",
  "keywords": [
    "pimcore",
    "pimcore-bundle",
    "translations",
    "pimcore-translations",
    "javascript-translations"
  ],
  "homepage": "https://github.com/instride-ch/PimcoreJsTranslationBundle",
  "authors": [
    {
      "name": "instride AG",
      "email": "connect@instride.ch",
      "homepage": "https://instride.ch",
      "role": "Digital Agency"
    }
  ],
  "require": {
    "php": ">=8.1",
    "matthiasmullie/minify": "^1.0",
    "pimcore/pimcore": "^10.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-symfony": "^1.3",
    "roave/security-advisories": "dev-main",
    "symplify/easy-coding-standard": "^12.1"
  },
  "autoload": {
    "psr-4": {
      "Instride\\Bundle\\PimcoreJsTranslationBundle\\": "src/PimcoreJsTranslationBundle/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Instride\\Bundle\\PimcoreJsTranslationBundle\\PimcoreJsTranslationBundle"
      ]
    },
    "branch-alias": {
      "dev-main": "2.0-dev"
    }
  }
}
